.top-main
    background: linear-gradient(130.41deg, #1F8058 -48.15%, #0D4A31 64.12%);
    position relative
    padding-top 20px

    @media  screen and (max-width 766px)
        padding-bottom 20px

    .titleProduct
        position absolute
        width 100%
        max-width 1280px
        display flex
        flex-direction column
        left 50%
        top 50%
        transform translate(-50%, -50%)
        z-index 5

        @media  screen and (max-width 766px)
            display none

        h4
            font-size 60px
            color #fff
            font-family Playfair
            font-weight 500
            letter-spacing 4px  

        p
            font-size 18px
            color #fff
            margin-top 25px
            font-family Montserrat

    .container
        position relative
        z-index 3

        ul
            li
                a
                    color #fff
                    transition all .2s linear

                    &:hover
                        color #000

        .dropdown
            ul
                li
                    a
                        color #000

        .logo
            background-image url('data:image/svg+xml;base64,')
            background-repeat no-repeat
            width 112px
            height 28px
            background-size cover
            margin-right 20px
            

    @media screen and (min-width 1024px)
        height 800px
    